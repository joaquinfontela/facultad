Object subclass: #Club	instanceVariableNames: 'nombre tarifaSocio tarifaSocioVIP recaudacionTotal socios sociosVIP cantidadDeSocios cantidadDeSociosVIP'	classVariableNames: ''	package: 'tp0.5'!!Club methodsFor: 'accessing' stamp: 'joaquinfontela 4/30/2020 23:13'!cantidadDeSocios		^ (cantidadDeSocios + cantidadDeSociosVIP).! !!Club methodsFor: 'accessing' stamp: 'joaquinfontela 4/30/2020 22:38'!recaudacionTotal		^ recaudacionTotal.! !!Club methodsFor: 'initialization' stamp: 'joaquinfontela 4/30/2020 23:12'!initialize		nombre := ''.	recaudacionTotal := 0.	cantidadDeSocios := 0.	cantidadDeSociosVIP := 0.	socios := Set new.	sociosVIP := Set new.! !!Club methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 23:37'!obtenerSocio: nombreSocio	| socioBuscado |		(self esSocioVIP: nombreSocio) ifTrue: [ 		socioBuscado := sociosVIP detect: [ :socio | (socio obtenerNombre) = nombreSocio ]		 ] ifFalse: [ 		socioBuscado := socios detect: [ :socio | (socio obtenerNombre) = nombreSocio ]			 ].		^ socioBuscado.		! !!Club methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 23:12'!agregarSocioVIP: nuevoSocio	sociosVIP add: nuevoSocio.	cantidadDeSociosVIP := cantidadDeSociosVIP + 1.		! !!Club methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 23:06'!nombresSocios		| nombresSocios |	nombresSocios := Set new.		sociosVIP do: [ :socioVIP | nombresSocios add: (socioVIP obtenerNombre) ].	socios do: [ :socio | nombresSocios add: (socio obtenerNombre) ].		^ nombresSocios.! !!Club methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 22:35'!establecerTarifaSocio: tarifa			tarifaSocio := tarifa.! !!Club methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 23:22'!esSocioVIP: nombreSocio	| esSocioVIP |		esSocioVIP:= sociosVIP anySatisfy: [ :socio | (socio obtenerNombre = nombreSocio) ].		^ esSocioVIP.	! !!Club methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 23:40'!cantidadPagadaPor: nombreSocio		| socio |		socio := self obtenerSocio: nombreSocio.		^ (socio cantidadPagada).! !!Club methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 23:39'!recibirPagoDe: nombreSocio	| socio |	socio := self obtenerSocio: nombreSocio.	(self esSocioVIP: nombreSocio) ifTrue: [ 		recaudacionTotal := recaudacionTotal + tarifaSocioVIP.		socio pagar: tarifaSocioVIP.		 ] ifFalse: [ 		recaudacionTotal := recaudacionTotal + tarifaSocio.			socio pagar: tarifaSocio.			 ]				! !!Club methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 23:11'!agregarSocio: nuevoSocio	socios add: nuevoSocio.	cantidadDeSocios := cantidadDeSocios + 1.		! !!Club methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 22:36'!establecerTarifaSocioVIP: tarifa			tarifaSocioVIP := tarifa.! !!Club methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 22:34'!establecerNombre: nuevoNombre		nombre := nuevoNombre.! !Object subclass: #Socio	instanceVariableNames: 'nombre cantidadPagada'	classVariableNames: ''	package: 'tp0.5'!!Socio methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 22:45'!obtenerNombre		^ nombre.! !!Socio methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 23:34'!pagar: cantidad	cantidadPagada := cantidadPagada + cantidad.! !!Socio methodsFor: 'as yet unclassified' stamp: 'joaquinfontela 4/30/2020 22:53'!establecerNombre: nuevoNombre		nombre := nuevoNombre.! !!Socio methodsFor: 'accessing' stamp: 'joaquinfontela 4/30/2020 23:29'!cantidadPagada		^ cantidadPagada.! !!Socio methodsFor: 'initialization' stamp: 'joaquinfontela 4/30/2020 23:29'!initialize 		nombre := ''.	cantidadPagada := 0.! !